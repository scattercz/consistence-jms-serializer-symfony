{
	"name": "scattercz/consistence-jms-serializer-symfony",
	"description": "Symfony Bundle integrating Consistence library and JMS Serializer with Symfony 5 support",
	"license": "MIT",
	"authors": [
		{
			"name": "Vašek Purchart",
			"email": "me@vasekpurchart.cz",
			"homepage": "http://vasekpurchart.cz"
		},
		{
			"name": "Zdeněk Novotný",
			"email": "scatter@scatter.cz",
			"homepage": "https://scatter.cz"
		}
	],
	"require": {
		"php": "~7.2",
		"consistence/consistence-jms-serializer": "~2.0",
		"jms/serializer-bundle": "^2.4",
		"symfony/config": "~3.4|~4.0|~5.0",
		"symfony/dependency-injection": "~3.4|~4.0|~5.0",
		"symfony/http-kernel": "~3.4|~4.0|~5.0",
		"symfony/yaml": "~3.4|~4.0|~5.0"
	},
	"require-dev": {
		"consistence/coding-standard": "3.7",
		"php-parallel-lint/php-console-highlighter": "0.5",
		"php-parallel-lint/php-parallel-lint": "1.2",
		"matthiasnoback/symfony-dependency-injection-test": "~4.0",
		"phpunit/phpunit": "8.5.5"
	},
	"autoload": {
		"psr-4": {
			"Consistence\\JmsSerializer\\SymfonyBundle\\": "src"
		},
		"classmap": [ "src" ]
	},
	"autoload-dev": {
		"psr-4": {
			"Consistence\\JmsSerializer\\SymfonyBundle\\": "tests"
		},
		"classmap": [ "tests" ]
	},
	"config": {
		"bin-dir": "bin"
	}
}
